<%@ val code: String %>

<div class="voucher">
    <legend>
        Voucher
    </legend>
    If you have a voucher code, please enter it here
    <form method="POST" action="/listing/voucher">
        <input type="text" name="voucherCode" size="12" value="${code}"/>
        <button type="submit" class="btn btn-default">Add code</button>
    </form>
</div>