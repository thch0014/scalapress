#parse("/admin/header.vm")


<h3>Search Results</h3>


<table class="table table-condensed table-hover">
    <tr>
        <th>Id</th>
        <th>Type</th>
        <th>Name</th>
        <th></th>
    </tr>
    #foreach ($result in $results)
        <tr>
            <td>$result.id()</td>
            <td>$result.t()</td>
            <td>$result.name()</td>
            <td>
                <a href="$result.editUrl()" role="button" class="btn btn-mini">
                    <i class="icon-pencil"></i></a>
                <a href="$result.viewUrl()" role="button" class="btn btn-mini">
                    <i class="icon-eye-open"></i></a>
            </td>
        </tr>
    #end
</table>



#parse("/admin/footer.vm")