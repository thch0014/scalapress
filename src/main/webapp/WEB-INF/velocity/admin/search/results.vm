#parse("/admin/header.vm")


<h1>Search Results</h1>


<table class="table table-condensed table-hover">
    <tr>
        <th>Id</th>
        <th>Type</th>
        <th>Name</th>
        <th></th>
    </tr>
    #foreach ($result in $page.java())
        <tr>
            <td>$result.id</td>
            <td>$result.type</td>
            <td>$result.name</td>
            <td>
                <a href="" role="button" class="btn btn-mini">
                    <i class="icon-pencil"></i></a>
                <a href="" role="button" class="btn btn-mini">
                    <i class="icon-eye-open"></i></a>
            </td>
        </tr>
    #end
</table>



#parse("/admin/footer.vm")