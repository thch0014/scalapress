#parse("/admin/header.vm")


<h3>Delivery Options</h3>


<table class="table table-condensed table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th></th>
    </tr>
    #foreach ($o in $options)
        <tr>
            <td>$o.id</td>
            <td>$o.name</td>
            <td><a href="$urlResolver.editDeliveryOption($o)" role="button" class="btn btn-mini"> <i
                    class="icon-pencil"></i>
            </a></td>
        </tr>
    #end
</table>


#parse("/admin/footer.vm")