#parse("/admin/header.vm")


<h1>Object Types</h1>


<table class="table table-condensed table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th></th>
    </tr>
    #foreach ($t in $types)
        <tr>
            <td>$t.id</td>
            <td>$t.name</td>
            <td>
                <a href="$urlResolver.typeEdit($t)" role="button" class="btn btn-mini"> <i class="icon-pencil"></i></a>
            </td>
        </tr>
    #end
</table>


#parse("/admin/footer.vm")