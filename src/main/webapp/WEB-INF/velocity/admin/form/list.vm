#parse("/admin/header.vm")


<h1>Forms</h1>


<table class="table table-condensed table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th></th>
    </tr>
    #foreach ($f in $forms)
        <tr>
            <td>$f.id</td>
            <td>$f.name</td>
            <td><a href="$urlResolver.editForm($f)" role="button" class="btn btn-mini"> <i class="icon-pencil"></i>
            </a></td>
        </tr>
    #end
</table>


#parse("/admin/footer.vm")