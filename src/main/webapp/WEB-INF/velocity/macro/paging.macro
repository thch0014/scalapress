#macro( paging $paging )
<ul class="pagination">
      #foreach ($count in $paging.range(5))
          #if ($count == $paging.pageNumber)
              #set($active="active")
          #else
              #set($active="")
          #end
          <li class="$active"><a href="$paging.url($count)">$count</a></li>
      #end
  </ul>
#end